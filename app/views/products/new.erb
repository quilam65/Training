<%= semantic_form_for @product do |f| %>
      <%= f.input :title%>
      <%= f.input :description %>
      <%= f.input :price, min: 1 %>
      <%= f.input :published %>
      <%= f.input :country, as: :select %>
      <%= f.input :level %>
      <%= f.input :category %>
      <%= f.action :submit %>
<% end %>
